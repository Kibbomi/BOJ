#include <stdio.h>
#include <algorithm>
using namespace std;

int main()
{
	int val;
	scanf("%d", &val);

	int num;
	scanf("%d", &num);
	
	while (num != 0)
	{
		if (num%val == 0)
			printf("%d is a multiple of %d.\n", num, val);
		else
			printf("%d is NOT a multiple of %d.\n", num, val);


		scanf("%d", &num);
	}
	return 0;
}

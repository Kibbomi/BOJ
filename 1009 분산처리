#include <stdio.h>
#include <algorithm>
using namespace std;

int main(void)
{
	
	int t;
	scanf("%d", &t);
	for (int i = 0; i < t; i++)
	{
		int a, b;
		scanf("%d %d", &a, &b);

		a = a % 10;
		int ret = 1;

		if (a == 0)
		{
			printf("%d\n", 10);
		}
		if (a == 1)
		{
			printf("%d\n", 1);
		}
		else if (a == 2)
		{
			b = b % 4;
			if (b == 0)b = 4;
			for (int j = 0; j < b; j++)
			{
				ret *= a;
				ret = ret % 10;
			}
			printf("%d\n", ret);
		}
		else if (a == 3)
		{
			b = b % 4;
			if (b == 0)b = 4;
			for (int j = 0; j < b; j++)
			{
				ret *= a;
				ret = ret % 10;
			}
			printf("%d\n", ret);
		}
		else if (a == 4)
		{
			b = b % 2;
			if (b == 0)b = 2;
			for (int j = 0; j < b; j++)
			{
				ret *= a;
				ret = ret % 10;
			}
			printf("%d\n", ret);
		}
		else if (a == 5)
		{
			printf("%d\n", 5);
		}
		else if (a == 6)
		{
			printf("%d\n", 6);
		}
		else if (a == 7)
		{
			b = b % 4;
			if (b == 0)b = 4;
			for (int j = 0; j < b; j++)
			{
				ret *= a;
				ret = ret % 10;
			}
			printf("%d\n", ret);
		}
		else if (a == 8)
		{
			b = b % 4;
			if (b == 0)b = 4;
			for (int j = 0; j < b; j++)
			{
				ret *= a;
				ret = ret % 10;
			}
			printf("%d\n", ret);
		}
		else if (a == 9)
		{
			b = b % 2;
			if (b == 0)b = 2;
			for (int j = 0; j < b; j++)
			{
				ret *= a;
				ret = ret % 10;
			}
			printf("%d\n", ret);
		}



	}

	return 0;
}
